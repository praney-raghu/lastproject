{
    "name": "ssntpl/neev",
    "description": "User to organisation associations for the Laravel 5 Framework",
    "keywords": ["Organisation","Invite"],
    "homepage": "http://ssntpl.com/neev",
    "license": "proprietary",
    "authors": [
        {
            "name": "Sambhav Aggarwal",
            "email": "sambhav@ssntpl.com"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "laravelcollective/html": "^5.5",
        "spatie/laravel-permission": "^2.7",
        "spatie/laravel-translatable": "^2.1",
        "spatie/laravel-translation-loader": "^2.1",
        "kwn/number-to-words": "~1.2.0",
        "florianv/laravel-swap": "^1.2",
        "php-http/guzzle6-adapter": "^1.1",
        "php-http/message": "^1.6",
        "geoip2/geoip2": "~2.0",
        "pragmarx/countries": "^0.5.6",
        "propaganistas/laravel-phone": "^4.0",
        "propaganistas/laravel-intl": "^1.2",
        "jenssegers/agent": "^2.6",
        "lavary/laravel-menu": "^1.7",
        "nwidart/laravel-modules": "^3.0",
        "laravel/passport": "^5.0"
    },
    "require-dev": {
        "barryvdh/laravel-translation-manager": "^0.4.2",
        "barryvdh/laravel-debugbar": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "Ssntpl\\Neev\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ssntpl\\Neev\\ServiceProvider"
            ],
            "aliases": {
                "Neev": "Ssntpl\\Neev\\Facades\\Neev",
                "i18n": "Ssntpl\\Neev\\Facades\\i18n"
            }
        }
    }
}
